{
  "name": "keyscroll",
  "version": "0.1.4",
  "description": "Simple keyframe animations that run while scrolling through the page.",
  "main": "dist/index.js",
  "umd:main": "dist/keyscroll.cjs.js",
  "module": "dist/keyscroll.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf ./node_modules && rm -rf ./dist",
    "start": "npm run dev",
    "dev": "echo Staring keyscroll library watch... && tsdx watch --target browser --write-to-file",
    "build": "tsdx build --target browser --write-to-file",
    "lint": "tsdx lint src test",
    "lint:fix": "tsdx lint src test --fix",
    "test": "tsdx test",
    "test:ci": "npm run test"
  }
}
